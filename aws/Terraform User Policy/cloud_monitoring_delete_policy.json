{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "cloudwatch",
            "Effect": "Allow",
            "Action": [
                "cloudwatch:DeleteAlarms",
                "logs:DeleteLogGroup"
            ],
            "Resource": [
                "*"
            ]
        },
        {
            "Sid": "cloudtrail",
            "Effect": "Allow",
            "Action": [
                "cloudtrail:StopLogging",
                "cloudtrail:DeleteTrail"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}