{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Statement1",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteBucket",
                "s3:DeleteBucketPolicy"
            ],
            "Resource": [
                "*"
            ]
        },
		{
			"Sid": "Statement2",
			"Effect": "Allow",
			"Action": ["s3:DeleteObjectVersion"],
			"Resource": ["arn:aws:s3:::mangoapps-tfbackend-bucket",
                         "arn:aws:s3:::mangoapps-tfbackend-bucket/*"]
		}
    ]
}